    <script src="/Scripts/plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>
    <script src="/Scripts/plugins/masked-input/masked-input.min.js"></script>
    <script src="/Scripts/plugins/bootstrap-timepicker/js/bootstrap-timepicker.min.js"></script>
