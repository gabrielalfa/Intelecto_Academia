    <script>
        $(document).ready(function () {
            App.init();
            FormPlugins.init();
            FormWizardValidation.init();
        });
    </script>

    <script>
        $(document).ready(function () {
            $("#atd").hide();
            $("#cdn").hide();
            $("#pfs").hide();
            $("#fnc").hide();
        $("#atd_btn").click(function () {
            $("#atd").toggle(200);
            $("#cdn").hide();
            $("#pfs").hide();
            $("#fnc").hide();
        });
        $("#cdn_btn").click(function () {
            $("#atd").hide();
            $("#cdn").toggle(200);
            $("#pfs").hide();
            $("#fnc").hide();
        });
        $("#pfs_btn").click(function () {
            $("#atd").hide(200);
            $("#cdn").hide(200);
            $("#pfs").toggle(200);
            $("#fnc").hide(200);
        });
        $("#fnc_btn").click(function () {
            $("#atd").hide(200);
            $("#cdn").hide(200);
            $("#pfs").hide(200);
            $("#fnc").toggle(200);
        }); 
    });
    </script>

    <script>
    $(document).ready(function () {
        $("li").click(function () {
            $(this).addClass("active");
        });
    });
    </script>






